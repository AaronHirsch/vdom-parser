  "peerDependencies": {
    "virtual-dom": "2.x"
  },
  "devDependencies": {
    "browserify": "^10.2.4",
    "chai": "^3.0.0",
    "mocha": "^2.2.5",
    "onchange": "^1.1.0"
  }
